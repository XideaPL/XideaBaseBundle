    },
    "require-dev": {
        "doctrine/orm": "*",
        "doctrine/doctrine-bundle": "*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": { "Xidea\\Bundle\\BaseBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}