{
    "name": "xideapl/base-bundle",
    "type": "symfony-bundle",
    "description": "Symfony XideaBaseBundle",
    "keywords": [],
    "homepage": "http://xidea.pl",
    "authors": [
        {
            "name": "Artur Pszczółka",
            "email": "artur.pszczolka@xidea.pl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
